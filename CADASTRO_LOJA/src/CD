package LOJA;
public class CD extends Produtos
{
    int numerodefaixa;
    
    public CD(int codigo){
        super(codigo);
    }
    
    public void Produtos2(String nome,double preco, int numerodefaixa){
        super.Produtos2(nome,preco);
        this.numerodefaixa = numerodefaixa;
    }
    
    public String toString(){
        return super.toString() + "/Numero de faixas:" + numerodefaixa;
    }
}
